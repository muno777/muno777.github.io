{
  "name": "example-with-tailwindcss",
  "type": "module",
  "scripts": {
    "portfolio-json": "ts-node scripts/generatePortfolioJson.ts",
    "watch-portfolio-json": "nodemon --watch public/img/portfolio --ext png,jpg,jpeg,gif,webp,txt --exec \"ts-node scripts/generatePortfolioJson.ts\"",
    "vinxi-dev": "vinxi dev",
    "dev": "npm-run-all -p watch-portfolio-json vinxi-dev",
    "build": "npm run portfolio-json && vinxi build",
    "start": "vinxi start"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "solid-js": "^1.9.5",
    "vinxi": "^0.5.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.7",
    "@types/node": "^24.2.1",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=22"
  }
}
